#!/usr/bin/env python2
# -*- coding:utf-8 -*-

import os
#check the PATH
for dir in [
        var for var in
        os.environ['PATH'].split(os.pathsep)
        if var not in os.environ.get('PATH_CAN_BE_MISSING', "").split(os.pathsep)
]:
    if not os.path.exists(dir):
        print "WARNING :",dir,"does not exist and yet it is in your path"
