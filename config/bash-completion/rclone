_rclone_config_providers()
{
    last_command="rclone_config_providers"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--acd-auth-url=")
    two_word_flags+=("--acd-auth-url")
