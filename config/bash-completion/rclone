_rclone_genautocomplete_bash()
{
    last_command="rclone_genautocomplete_bash"

    command_aliases=()

    commands=()

    flags=()
    two_word_flags=()
    local_nonpersistent_flags=()
    flags_with_completion=()
    flags_completion=()

    flags+=("--help")
    flags+=("-h")
    local_nonpersistent_flags+=("--help")
    flags+=("--acd-auth-url=")
    two_word_flags+=("--acd-auth-url")
