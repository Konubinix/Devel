#!/bin/bash

pkill -9 fluxbox
fluxbox &

fbpid=$!


#gnome-terminal --maximize &
setxkbmap -option ctrl:swapcaps
guake &
volwheel &
#Â wallpaper-tray &
numlockx
~/bin/notify_daemon.py &


sleep 4
{
    killall conky
    nm-applet &
    conky -d -c ~/.conky.d/conky1
	gnome-setting-daemon
	nautilus --no-default-window
} &

wait $fbpid
