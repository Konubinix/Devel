<gconfentryfile>
  <entrylist base="/desktop/gnome/session">
    <entry>
      <key>default_session</key>
      <schema_key>/schemas/desktop/gnome/session/default_session</schema_key>
      <value>
        <list type="string">
            <value>
              <string>gnome-settings-daemon</string>
            </value>
        </list>
      </value>
    </entry>
    <entry>
      <key>idle_delay</key>
      <schema_key>/schemas/desktop/gnome/session/idle_delay</schema_key>
      <value>
        <int>5</int>
      </value>
    </entry>
    <entry>
      <key>required_components_list</key>
      <schema_key>/schemas/desktop/gnome/session/required_components_list</schema_key>
      <value>
        <list type="string">
            <value>
              <string>panel</string>
            </value>
            <value>
              <string>filemanager</string>
            </value>
        </list>
      </value>
    </entry>
    <entry>
      <key>required_components/filemanager</key>
      <schema_key>/schemas/desktop/gnome/session/required_components/filemanager</schema_key>
      <value>
        <string>nautilus</string>
      </value>
    </entry>
    <entry>
      <key>required_components/panel</key>
      <schema_key>/schemas/desktop/gnome/session/required_components/panel</schema_key>
      <value>
        <string>gnome-panel</string>
      </value>
    </entry>
    <entry>
      <key>required_components/windowmanager</key>
      <schema_key>/schemas/desktop/gnome/session/required_components/windowmanager</schema_key>
      <value>
        <string>fluxbox</string>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
