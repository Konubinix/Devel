<gconfentryfile>
  <entrylist base="/apps/notification-daemon">
    <entry>
      <key>default_sound</key>
      <schema_key>/schemas/apps/notification-daemon/default_sound</schema_key>
      <value>
        <string></string>
      </value>
    </entry>
    <entry>
      <key>popup_location</key>
      <schema_key>/schemas/apps/notification-daemon/popup_location</schema_key>
      <value>
        <string>bottom_right</string>
      </value>
    </entry>
    <entry>
      <key>sound_enabled</key>
      <schema_key>/schemas/apps/notification-daemon/sound_enabled</schema_key>
      <value>
        <bool>true</bool>
      </value>
    </entry>
    <entry>
      <key>theme</key>
      <schema_key>/schemas/apps/notification-daemon/theme</schema_key>
      <value>
        <string>slider</string>
      </value>
    </entry>
  </entrylist>
</gconfentryfile>
